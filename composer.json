{
    "name": "kalfheim/session-tokens",
    "description": "Unique, revokable \"remember me\" tokens for Laravel",
    "keywords": ["laravel", "authentication"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristoffer Alfheim",
            "email": "kristoffer.alfh@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alfheim\\SessionTokens\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alfheim\\SessionTokens\\Tests\\": "./tests"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.5",
        "illuminate/console": "~5.5",
        "illuminate/contracts": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/support": "~5.5",
        "symfony/http-foundation": "~3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "~6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alfheim\\SessionTokens\\SessionTokensServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
